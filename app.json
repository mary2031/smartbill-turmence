{
  "expo": {
    "name": "SmartBill",
    "slug": "bolt-expo-nativewind",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon1.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "59d06fc2-3c7d-420f-aeb8-84f99ed6719c"
      }
    },
    "android": {
      "package": "com.ezizhan1.billapp"
    }
  }
}
